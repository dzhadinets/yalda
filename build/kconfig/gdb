menu "GDB"

config YALDA_GDB_FROM_TOOLCHAIN
  bool "Use GDB from toolchain with toolchain prefix"
  default y

config YALDA_GDB_PARAMETERS
  string "Launch parameters"
  default "-tui"
  help
    GDB parameters

config YALDA_GDB_EXTRAINIT
  string "Additional init script"
  default ""
  help
    GDB script for your project ie list of stored breakpoints

endmenu
